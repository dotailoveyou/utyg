import pygame

pygame.init()
win = pygame.display.set_mode((800, 800))

while True:
    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            exit()

    win.fill((255, 0, 225))
    pygame.draw.line(win,(160, 82, 45),(0,50),(315,50),6)
